{
  "name": "cellular-automata-react",
  "version": "1.3.2",
  "description": "Cellular Automata grid component capable of running your own automata variants, includes a library containing Conways game of life preset and its methods.",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "lib/index.es.js",
  "scripts": {
    "build": "rm -rf lib && rollup -c",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "./node_modules/.bin/eslint .",
    "release": "standard-version",
    "t:u": "jest ./src/__tests__ --coverage"
  },
  "keywords": [
    "cellular",
    "automata",
    "react",
    "conways",
    "game",
    "of",
    "life",
    "component",
    "typescript",
    "automaton"
  ],
  "author": "Craig White",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigwh10/cellular-automata-react"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "grunt-bump": "^0.8.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "prettier": "2.5.1",
    "rollup": "^2.61.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "/lib"
  ],
  "dependencies": {
    "zustand": "^4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
